{
  "version": 2,
  "builds": [
    {
      "src": "html/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/html/index.html"
    },
    {
      "src": "/html/(.*)",
      "dest": "/html/$1"
    },
    {
      "src": "^/assets/(.*)$",
      "dest": "/assets/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "^/css/(.*)$",
      "dest": "/css/$1"
    },
    {
      "src": "^/js/(.*)$",
      "dest": "/js/$1"
    }
  ]
}